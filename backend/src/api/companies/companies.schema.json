{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Company",
    "description": "The Company",
    "required": [
        "name",
        "logo_url",
        "description",
        "website_url",
        "email",
        "address_id"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the Company."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the Comapny."
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo of Company."
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description of the Company."
        },
        "website_url": {
            "$id": "#/properties/website_url",
            "type": "string",
            "title": "The website of the Company."
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email of the Company."
        },
        "address_id": {
            "$id": "#/properties/address_id",
            "type": "number",
            "title": "The id of the address."
        }
    }
}